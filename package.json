{
    "name": "doz-router",
    "version": "1.12.4",
    "description": "The doz router",
    "main": "dist/bundle.js",
    "type": "module",
    "scripts": {
        "build": "rollup --config rollup.config.js",
        "test": "mocha test",
        "vite:dev": "vite --force",
        "vite:build": "vite build",
        "vite:preview": "vite preview",
        "release:before": "npm run-script test",
        "release:major": "npm run-script build && npm run-script release:publish",
        "release:minor": "npm run-script build && npm run-script release:publish",
        "release:patch": "npm run-script build && npm run-script release:publish",
        "release:publish": "echo git commit -a -m BUILD && version-to-tag.sh && npm publish"
    },
    "keywords": [
        "doz",
        "routing",
        "routes"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dozjs/doz-router.git"
    },
    "bugs": {
        "url": "https://github.com/dozjs/doz-router/issues"
    },
    "homepage": "https://github.com/dozjs/doz-router#readme",
    "author": "Fabio Ricali",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.3",
        "doz": "github:dozjs/doz#hyd",
        "mocha": "^10.2.0",
        "rollup": "^2.79.1",
        "rollup-plugin-version-injector": "^1.3.3",
        "vite": "^4.4.9"
    }
}
