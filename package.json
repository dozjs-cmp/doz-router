{
    "name": "doz-router",
    "version": "1.5.0",
    "description": "Routing for DOZ",
    "main": "dist/bundle.js",
    "scripts": {
        "start": "webpack-dev-server --open",
        "build": "webpack --progress --mode=production",
        "test": "mocha test",
        "release:before": "npm run-script test",
        "release:major": "webpack --env.major --mode=production && npm run-script release:publish",
        "release:minor": "webpack --env.minor --mode=production && npm run-script release:publish",
        "release:patch": "webpack --env.patch --mode=production && npm run-script release:publish",
        "release:publish": "echo git commit -a -m BUILD && version-to-tag.sh && npm publish",
        "test-hmr-watch": "parcel watch test/HMR/index.js --out-file bundle --no-cache --out-dir test/HMR/public/dist",
        "test-hmr-build": "parcel build test/HMR/index.js --out-file bundle --no-cache --out-dir test/HMR/public/dist --public-url ./",
        "test-history-watch": "parcel watch test/server-history/client/app.js --out-file bundle --no-cache --out-dir test/server-history/public/dist",
        "test-history-build": "parcel build test/server-history/client/app.js --out-file bundle --no-cache --out-dir test/server-history/public/dist --public-url ./",
        "test-history-start-server": "node test/server-history/index.js",
        "test-ssr-watch": "parcel watch test/SSR/client/app.js --out-file bundle --no-cache --out-dir test/SSR/public/dist --public-url ./",
        "test-ssr-build": "parcel build test/SSR/client/app.js --out-file bundle --no-cache --out-dir test/SSR/public/dist --public-url ./",
        "test-ssr-start-server": "node test/SSR/index.js"
    },
    "keywords": [
        "doz",
        "routing",
        "routes"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dozjs/doz-router.git"
    },
    "bugs": {
        "url": "https://github.com/dozjs/doz-router/issues"
    },
    "homepage": "https://github.com/dozjs/doz-router#readme",
    "author": "Fabio Ricali",
    "license": "MIT",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "bejs": "^2.0.1",
        "chai": "^4.1.2",
        "css-loader": "^0.28.11",
        "doz": "^1.6.6",
        "doz-ssr": "^1.0.1",
        "hard-source-webpack-plugin": "^0.12.0",
        "ignore-styles": "^5.0.1",
        "jsdoc-to-markdown": "^4.0.1",
        "jsdom": "^11.6.2",
        "jsdom-global": "^3.0.2",
        "koa": "^2.5.2",
        "koa-body": "^4.0.4",
        "koa-router": "^7.4.0",
        "koa-static": "^5.0.0",
        "mocha": "^5.0.4",
        "parcel-bundler": "^1.9.4",
        "pushstate-server": "^3.0.1",
        "style-loader": "^0.21.0",
        "uglifyjs-webpack-plugin": "^1.2.6",
        "unminified-webpack-plugin": "^2.0.0",
        "webpack": "^4.16.5",
        "webpack-auto-inject-version": "^1.1.0",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.5"
    },
    "dependencies": {}
}
